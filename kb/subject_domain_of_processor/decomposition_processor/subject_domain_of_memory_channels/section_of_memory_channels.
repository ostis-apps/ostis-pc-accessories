
subject_domain_of_memory_channels
    =>nrel_main_idtf:[Предметная область количества каналов памяти] (* <-lang_ru;;*);
    =>nrel_main_idtf:[Subject domain memory channels] (* <-lang_en;;*);
	<-subject_domain; 
	<-ssubject_domain_of_processor; 
    ->rrel_maximum_studied_object_class:
        concept_memory_support;
    ->rrel_not_maximum_studied_object_class:
        concept_single_channel;
        concept_dual_channel;
        concept_quad_channel;
        concept_six_channel;;

